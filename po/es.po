msgid "Modules"
msgstr "Drivers"

#. TXT_INFO_PCI
msgid "PCI"
msgstr "PCI"

#. TXT_INSERT_DISK
msgid "Please insert the disk and press ENTER"
msgstr "Inserte el disco y presione ENTER"

#. TXT_ERROR_READ_DISK
msgid "Error reading from floppy disk!"
msgstr "¡Error mientras se leía del floppy!"

#. TXT_ENTER_ROOT_FS
msgid "Please enter your root partition (eg. /dev/sda3)"
msgstr "Introduzca su partición raíz (p. ej. /dev/hda1)"

#. TXT_INVALID_ROOT_FS
msgid "Invalid root device!"
msgstr "Dispositivo raíz no válido"

#. TXT_INVALID_INPUT
msgid "Invalid input!"
msgstr "¡Entrada no válida!"

#. TXT_INPUT_IPADDR
msgid "Please enter your IP address"
msgstr "Introduzca su dirección IP"

#. TXT_INPUT_NETMASK
msgid "Please enter your netmask. For a normal class C network this is usually 255.255.255.0"
msgstr "Introduzca su máscara de red. En una red de clase C, normalmente es 255.255.255.0"

#. TXT_INPUT_GATEWAY
msgid "If you need a gateway to reach the server, please enter the IP address of the gateway. If you do not need a gateway, just enter your own IP address."
msgstr "Si necesita un gateway para conectar con su servidor, introduzca la IP del gateway. Si no lo necesita, introduzca su IP."

#. TXT_INPUT_NAMED
msgid "Please enter the IP address of your name server. If you don't use a name server, press ESC."
msgstr "Introduzca la IP de su servidor de nombres o DNS. Si no necesita uno, pulse ESC."

#. TXT_FOUND_HOST
#, c-format
msgid ""
"The following data have been retrieved:\n"
"\n"
"IP address: %s\n"
"Name: %s\n"
"Domain: %s"
msgstr ""
"Se obtuvo la siguiente información:\n"
"\n"
"Dirección IP: %s\n"
"Nombre: %s\n"
"Dominio: %s"

#. TXT_ERROR_CONF_NET
msgid "An error occurred during the network configuration. Probably your network card was not recognized by the kernel!"
msgstr "Ocurrió un error mientras se configuraba la red. ¡Posiblemente su tarjeta de red no fue reconocida por el kernel!"

#. TXT_INPUT_SERVER
msgid "Please enter the IP address of the NFS server."
msgstr "Introduzca la dirección IP del servidor NFS."

#. TXT_INPUT_FTPSERVER
msgid "Please enter the IP address of the FTP server."
msgstr "Introduzca la dirección del servidor FTP."

#. TXT_INPUT_DIR
msgid "Please enter the directory on the server."
msgstr "Especifique el directorio en el servidor."

#. TXT_ENTER_PARAMS
#, c-format
msgid "Please enter parameters for \"%s\"."
msgstr "Introduzca los parámetros para \"%s\"."

#. TXT_TRY_NFS_MOUNT
#, c-format
msgid ""
"Trying to mount the directory\n"
"\n"
"\"%s:%s\"..."
msgstr ""
"Intentando montar el directorio\n"
"\n"
"\"%s:%s\"..."

#. TXT_ERROR_NFSMOUNT
#, c-format
msgid ""
"An error occurred while trying to mount the nfs directory.\n"
"\n"
"Reason given by server: %s"
msgstr ""
"Ocurrió un error mientras se montaba el directorio NFS.\n"
"\n"
"Razón proporcionada por el servidor: %s"

#. TXT_CHOOSE_KEYMAP
msgid ""
"Please choose a keyboard map.\n"
"YaST will offer you additional keyboard tables later on."
msgstr ""
"Escoja un tipo de teclado.\n"
"YaST le permitirá escoger otros tipos más adelante."

#. TXT_DELETE_MODULE
msgid "Choose the module to be unloaded."
msgstr "Seleccione el driver a descargar."

#. TXT_NO_MODULES
msgid "Currently there are no modules loaded!"
msgstr "¡Actualmente no hay drivers cargados!"

#. TXT_AUTO_RUNNING
#, c-format
msgid "Autoprobing (%s)...      "
msgstr "Autodetectando (%s)...   "

#. TXT_WANT_MORE_SCSI
msgid "Do you want to search for additional SCSI host adaptors?"
msgstr "¿Desea buscar más adaptadores SCSI?"

#. TXT_WANT_MORE_NET
msgid "Do you want to search for additional network cards?"
msgstr "¿Desea buscar más tarjetas de red?"

#. TXT_WANT_MORE_CDROM
msgid "Do you want to search for additional CD-ROM drives?"
msgstr "¿Desea buscar más unidades de CD-ROM?"

#. TXT_NET_ETH0
msgid "ethernet network card"
msgstr "Tarjeta de red ethernet"

#. TXT_NET_PLIP
msgid "parallel port"
msgstr "Puerto paralelo"

#. TXT_NET_TR0
msgid "token ring network card"
msgstr "Tarjeta de red Token Ring"

#. TXT_NET_ARC0
msgid "arcnet network card"
msgstr "Tarjeta de red Arcnet"

#. TXT_NET_FDDI
msgid "FDDI network card"
msgstr "Tarjeta de red FDDI"

#. TXT_NET_HIPPI
msgid "HIPPI network card"
msgstr "Tarjeta de red HIPPI"

#. TXT_CHOOSE_NET
msgid "Please choose the network device."
msgstr "Escoja el dispositivo de red."

#. TXT_INPUT_PLIP_IP
msgid "Please enter the IP address of the PLIP partner."
msgstr "Escriba la direccción IP de su par PLIP."

#. TXT_NO_PCMCIA
msgid "The PCMCIA chipset could not be detectedautomatically. Please specify manually."
msgstr "El chipset PCMCIA no ha podido detectarse automáticamente. Por favor, especifíquelo manualmente."

#. TXT_FOUND_PCMCIA
#, c-format
msgid ""
"Found PCMCIA chipset \"%s\"!\n"
"\n"
"Please enter parameters for PCMCIA core modules"
msgstr ""
"Se encontró el chipset PCMCIA \"%s\"!\n"
"\n"
"Introduzca los parámetros para el driver de PCMCIA"

#. TXT_PCMCIA_PARAMS
#, c-format
msgid "Please enter specific parameters for chipset \"%s\""
msgstr "Escriba los parámetros específicos del chipset \"%s\""

#. TXT_START_CARDMGR
msgid "Starting card manager..."
msgstr "Iniciando administrador de tarjetas..."

