#. TXT_LOAD_MODULES
#, c-format
msgid "Load %s modules"
msgstr "Cargar módulos %s"

#. TXT_LOAD_FS
msgid "Load file system modules"
msgstr "Cargar módulos de sistema de ficheros"

#. TXT_LOAD_OTHER
msgid "Load unknown modules"
msgstr "Cargar módulos desconocidos"

# %s is e.g. ext3 or reiserfs
#. TXT_FILE_SYSTEM
#, c-format
msgid "%s file system found."
msgstr "Encontrado sistema de ficheros %s"

#. TXT_CONTINUE
msgid "Continue"
msgstr "Continuar"

#. TXT_ABORT
msgid "Abort"
msgstr "Abortar"

#. TXT_NO_NEW_MODULES
msgid "No new modules found."
msgstr "No se encontraron mñodulos nuevos."

#. TXT_CHOOSE_PARTITION
msgid "Please choose the harddisk partition."
msgstr "A continuación elija la partición del disco duro."

#. TXT_CHOOSE_ROOT_FS
msgid "Please choose the root partition."
msgstr "Por favor introduzca la partición raiz (root)."

#. TXT_CHOOSE_SWAP
msgid "Please choose the swap partition."
msgstr "Por favor seleccione la partición de intercambio (swap)."

#. TXT_LOW_MEMORY1
msgid "There is not enough memory to load all data."
msgstr "No hya suficiente memoria para cargar todos los datos."

#. TXT_LOW_MEMORY2
msgid "Your computer doesn't have enough memory to run YaST."
msgstr "Su computadora no dispone de memoria suficiente para ejecutar YaST."

#. TXT_ADD_SWAP
msgid "To continue, activate some swap space."
msgstr "Para continuar, active más espacio de intercambio (swap)."

#. TXT_ERROR_SWAP
msgid "Error activating swap space."
msgstr "Error al activar el espacio de intercambio (swap)."

#. TXT_ENTER_SWAP
msgid "Please enter the swap partition (eg. /dev/sda2)"
msgstr "Introduzca la partición de intercambio (p.ej. /dev/sda2)"

#. TXT_ENTER_USER
#, c-format
msgid "Please enter the user name under which you want to access the %s server"
msgstr ""

#. TXT_ENTER_PASSWORD
#, c-format
msgid "Please enter the password for the %s server"
msgstr ""

