#. TXT_ASK_DHCP
#, c-format
msgid "Automatic configuration via %s?"
msgstr "¿Configurar automaticamente via %s?"

# %s is either BOOTP or DHCP
#. TXT_SEND_DHCP
#, c-format
msgid "Sending %s request..."
msgstr "Enviando petición %s..."

# %s is either BOOTP or DHCP
#. TXT_ERROR_DHCP
#, c-format
msgid "%s configuration failed."
msgstr "Configuración %s ha fallado."

#. TXT_DHCP_VS_BOOTP
msgid "Use DHCP instead of BOOTP?"
msgstr "¿Usar DHCP en vez de BOOTP?"

#. TXT_MODDISK0
msgid "Please insert a modules disk."
msgstr "Por favor introduzca el disquette de módulos."

#. TXT_MODDISK1
#, c-format
msgid "Please insert modules disk %d."
msgstr "Por favor introduzca el disquette de módulos %d."

#. TXT_MODDISK2
msgid "You'll find instructions on how to create it in disks/README on CD1 or DVD."
msgstr "Encontrará instrucciones sobre cómo crear uno en disks/README en el CD1 o DVD."

#. TXT_CHOOSE_NETSOURCE
msgid "Please choose the network protocol"
msgstr "A continuación elija el protocolo de red."

# %s is NFS, FTP, HTTP or SMB
#. TXT_INPUT_NETSERVER
#, c-format
msgid "Please enter the IP address of the %s server."
msgstr "Introduzca la dirección IP del servidor %s."

# %s is FTP or HTTP
#. TXT_TRY_REACH_SERVER
#, c-format
msgid "Trying to connect to the %s server..."
msgstr "Intentando conectar al servidor %s..."

#. TXT_NET
msgid "Network"
msgstr "Red"

